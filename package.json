{
  "name": "home",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "serve -s build -l 3000",
    "analyze": "source-map-explorer 'build/*.js'",
    "prod:server:build": "cross-env NODE_ENV=production webpack --progress=false --config webpack/webpack.server.config.js",
    "prod:client:build": "cross-env NODE_ENV=production webpack --progress=false --config webpack/webpack.client.config.js",
    "prod:build": "concurrently 'yarn prod:server:build' 'yarn prod:client:build'",
    "prod:run": "cross-env NODE_ENV=production node ./build_server/server",
    "prod": "yarn prod:build && yarn prod:run",
    "dev:server": "cross-env NODE_ENV=development webpack --config webpack/webpack.server.config.js",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config webpack/webpack.client.config.js",
    "generate": "yarn run graphql-codegen --config config/codegen.js",
    "dev": "concurrently 'yarn dev:server' 'yarn dev:client'"
  },
